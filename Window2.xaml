<Window x:Class="Bank_Management_System.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bank_Management_System"
        mc:Ignorable="d"
        Title="Window2" Height="654" Width="913">
    <Grid x:Name="rdo_male" Margin="0,0,10,-6">
        <TextBox x:Name="txt_accno" HorizontalAlignment="Left" Margin="371,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" Background="#FF76A7F8" Height="26" FontSize="16"/>
        <Label Content="Account No" HorizontalAlignment="Left" Margin="178,108,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold"/>
        <Label Content="Address" HorizontalAlignment="Left" Margin="178,510,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" RenderTransformOrigin="0.501,-0.129" FontSize="16" FontWeight="Bold"/>
        <Label Content="Name&#xD;&#xA;" HorizontalAlignment="Left" Margin="178,156,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" Height="26" FontSize="16" FontWeight="Bold"/>
        <Label Content="Marrid" HorizontalAlignment="Left" Margin="178,296,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold"/>
        <Label Content="Gender" HorizontalAlignment="Left" Margin="178,252,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold"/>
        <Label Content="NID" HorizontalAlignment="Left" Margin="178,202,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold"/>
        <Label Content="District" HorizontalAlignment="Left" Margin="178,346,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold"/>
        <Label Content="Job" HorizontalAlignment="Left" Margin="178,436,0,0" VerticalAlignment="Top" Width="166" Background="#FFF09D9D" FontSize="16" FontWeight="Bold" RenderTransformOrigin="0.527,-0.747"/>
        <TextBox x:Name="txt_name" HorizontalAlignment="Left" Margin="371,156,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" Background="#FF76A7F8" Height="26" FontSize="16"/>
        <TextBox x:Name="txt_nid" HorizontalAlignment="Left" Margin="371,202,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" Background="#FF76A7F8" Height="26" FontSize="16"/>
        <TextBox x:Name="txt_address" HorizontalAlignment="Left" Margin="371,515,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="340" Background="#FF76A7F8" Height="26" FontSize="16"/>
        <RadioButton Content="Male" HorizontalAlignment="Center" Margin="0,253,0,0" VerticalAlignment="Top" Cursor="" GroupName="Gender" Height="12" Width="78"/>
        <RadioButton x:Name="rdo_female" Content="Female" HorizontalAlignment="Left" Margin="541,250,0,0" VerticalAlignment="Top" Cursor="" GroupName="Gender" IsEnabled="False" Height="15" Width="95"/>
        <CheckBox x:Name="chk_married" Content="Yes" HorizontalAlignment="Left" Margin="412,296,0,0" VerticalAlignment="Top" Height="25" Width="62"/>
        <CheckBox x:Name="chk_unmarried" Content="No" HorizontalAlignment="Left" Margin="541,296,0,0" VerticalAlignment="Top" Height="25" Width="62"/>
        <ListBox x:Name="lstd_district" HorizontalAlignment="Left" Height="61" Margin="371,342,0,0" VerticalAlignment="Top" Width="242">
            <ListBoxItem Content="Dhaka" />
            <ListBoxItem Content="Khulna" />
            <ListBoxItem Content="barishal"/>
            <ListBoxItem Content="Chattagong"/>
            <ListBoxItem Content="Rajshahi" />
            <ListBoxItem Content="Rangpur" />
            <ListBoxItem Content="mymansingh"/>
            <ListBoxItem Content="Sylhet"/>
        </ListBox>
        <ComboBox x:Name="cmb_box" HorizontalAlignment="Left" Margin="383,442,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Dhaka" IsSelected="True"/>
            <ComboBoxItem Content="Khulna"/>
            <ComboBoxItem Content="Barisal"/>
            <ComboBoxItem Content="Rajshahi"/>
            <ComboBoxItem Content="Rangpur"/>
            <ComboBoxItem Content="Sylhet"/>
            <ComboBoxItem Content="Chittagong"/>
            <ComboBoxItem Content="Mymaninghs"/>
        </ComboBox>
        <Label Content="New Customer" HorizontalAlignment="Center" Margin="0,24,0,0" VerticalAlignment="Top" Width="274" Height="59" Background="#FF91DD70" FontWeight="Bold" FontSize="36"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="784,556,0,0" VerticalAlignment="Top" Height="57" Width="90" Background="#FF57D7BB" FontWeight="Bold" FontSize="24" FontFamily="Sitka Text"/>
    </Grid>
</Window>
